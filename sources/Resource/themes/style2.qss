QWidget{
    background: white;
}
QTableWidget {
    border: 1px solid #4580fa;
    background-color:white;
    font-size: 11pt;
}
QTableWidget::item:selected {
    background: #e1f0fd;
    color: #000000;
}
QTableWidget>QHeaderView::section {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,   stop:0 rgba(60, 86, 162, 255), stop:1 rgba(150,250,250, 255));
    color: white;
    height: 25;
    border-left: none;
    border-top:none;
    border-bottom: none;
    border-right-width: 1px;
    border-right-color: white;
    border-right-style: solid;
    font-family: "KP CheonRiMa";
    font-size: 11pt;
    text-align: center;
    padding: 0px 0px 0px 3px;
}
QTabWidget {
    background: rgb(255, 255, 255);
    border: 2px solid #4580fa;
    font-family: "KP CheonRiMa";
    font-size: 11pt;
    text-align: center;
}
QTabWidget::pane { /* The tab widget frame */
     border-top: 2px solid #4580fa;
 }
QTabBar::tab {
	font: 11pt;
	border-top:2px solid #4580fa; 
	background: rgb(255, 255, 255);
            min-width: 80px;
	min-height: 16px;
            padding: 5px;
}
QTabBar::tab:!selected {
	color:rgb(255,255,255);
	background: #4580fa;
            margin-bottom: 0px; 
}
QTabBar::tab:selected {
	color:blue;
}
QTabBar::tab:disabled {
    color: none;
}
QMessageBox{
   background-color:white;
}
QToolTip{
    font: 20pt;
}
QToolBox
{
	background-color:white;
}
QToolBox::tab {
    background: #4580fa;
    color: white;
}
QToolBox::tab:selected { /* italicize selected tabs */
	background:blue;
    font-weight: bold;
    color: white;
}
